<div class="container" style="min-width:640px;">
	<div class="row-fluid">
		<div class="span12 custom-block">

<% if notice %>
<div class="alert alert-success fade in" style="margin-top: -20px; margin-bottom: 25px;">
    <a class="close" data-dismiss="alert" href="#">&times;</a>
    <%= notice %>
</div>
<% end %>

<% if alert %>
<div class="alert alert-success fade in" style="margin-top: -20px; margin-bottom: 25px">
    <a class="close" data-dismiss="alert" href="#">&times;</a>
<%= alert %>
</div>
<% end %>

	<%= render :partial => 'show'%>